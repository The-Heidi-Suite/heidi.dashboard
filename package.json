{
  "name": "kiel-web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=24.11.0",
    "yarn": ">=1.22.0",
    "npm": "Please use `yarn`."
  },
  "engineStrict": true,
  "scripts": {
    "dev": "vite",
    "mock": "vite --mode mock",
    "build": "tsc -b && vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "preview": "vite preview",
    "format": "prettier \"src/**/*.{ts,tsx,json,css,scss,md}\" --write",
    "prepare": "husky install",
    "check:i18n": "node scripts/validate-translations.js",
    "gen:i18n-schema": "npm run check:i18n && node scripts/generate-translation-schema.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/query-async-storage-persister": "^5.90.7",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-persist-client": "^5.90.7",
    "axios": "^1.13.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.6.0",
    "immer": "^10.2.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lucide-react": "^0.548.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-i18next": "^16.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.9.4",
    "react-simple-wysiwyg": "^3.4.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.2",
    "tailwind-scrollbar": "^4.0.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "msw": "^2.11.6",
    "path": "^0.12.7",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx,json}": [
      "yarn run check:i18n",
      "yarn run format",
      "yarn run lint --fix"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
